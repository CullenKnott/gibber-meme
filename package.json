{
  "name": "gibber-meme",
  "version": "1.0.0",
  "description": "real-time direct messaging app",
  "main": "server/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server/server.js",
    "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm start",
